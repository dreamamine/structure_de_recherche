{% extends('::base.html.twig') %}




{% block body %}
    
    
    
    <div class="responsive-table-line" style="border: 5px solid #ccc;
         margin:0px auto;
         max-width:800px;
         border-collapse: collapse;">

     <table class="table table-bordered table-body-center" >
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ researchTeam.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ researchTeam.name }}</td>
            </tr>
            <tr>
                <th>Productionindex</th>
                <td>{{ researchTeam.productionIndex }}</td>
            </tr>
            <tr>
                <th>membre</th>
                <td>{{ researchTeam.membre }}</td>
            </tr>
            
            
        </tbody>
    </table>

     <ul>
        <a class="btn btn-warning btn-sm" href="{{ path('researchteam_edit', { 'id': researchTeam.id }) }}">
            <i class="fa fa-cog"></i> Edit</a>
        <a class="btn btn-danger btn-sm" href="{{ path('researchteam_delete', { 'id': researchTeam.id }) }}">
            <i class="fa fa-trash-o fa-lg"></i> Delete </a>
        
               
        <a class="btn btn-primary" href="{{ path('researchteam_index') }}" aria-label="Skip to main navigation">
            <i class="fa fa-bars" aria-hidden="true"></i>
        </a>
        
           
            
    </ul>
        </div>
{% endblock %}
