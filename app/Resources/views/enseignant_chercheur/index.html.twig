{% extends('::base.html.twig') %}

{% block javascripts %}
    {{ parent() }}
    <script>
        $(function () {
            $("#list").DataTable();
        });
    </script>
{% endblock %}

{% block body %}
    <div class="box box-info box-solid">
        <div class="box-header with-border">
            <h3 class="box-title text-uppercase">Liste des enseignant_chercheur LGM</h3>
            <span class="pull-right">
                <a href="{{ path('enseignant_chercheur_new') }}" class="btn btn-info text-uppercase"><i class="fa fa-plus"></i> Ajouter</a>
            </span>
        </div>

        <div class="box-body">
            <div id="example1_wrapper" class="dataTables_wrapper form-inline dt-bootstrap">
                <div class="row">
                    <div class="col-sm-12">
                        <table id="list" class="table table-bordered table-striped dataTable" role="grid" aria-describedby="example1_info">
                            <thead>
                                <tr role="row">
                                    <th class="sorting_asc" tabindex="0" aria-controls="example1" rowspan="1" colspan="1" style="width: 246px;" aria-label="ID: activate to sort column descending" aria-sort="ascending">ID</th>
                                    <th class="sorting" tabindex="0" aria-controls="example1" rowspan="1" colspan="1" style="width: 301px;" aria-label="Cinenseigch: activate to sort column ascending">Cinenseigch</th>
                                    <th class="sorting" tabindex="0" aria-controls="example1" rowspan="1" colspan="1" style="width: 301px;" aria-label="Nom: activate to sort column ascending">Nom</th>
                                    <th class="sorting" tabindex="0" aria-controls="example1" rowspan="1" colspan="1" style="width: 268px;" aria-label="Prenom: activate to sort column ascending">Prenom</th>
                                    <th class="sorting" tabindex="0" aria-controls="example1" rowspan="1" colspan="1" style="width: 268px;" aria-label="Datenaiss: activate to sort column ascending">Datenaiss</th>
                                    <th class="sorting" tabindex="0" aria-controls="example1" rowspan="1" colspan="1" style="width: 301px;" aria-label="Lieunaiss: activate to sort column ascending">Lieunaiss</th>
                                    <th class="sorting" tabindex="0" aria-controls="example1" rowspan="1" colspan="1" style="width: 301px;" aria-label="Sexe: activate to sort column ascending">Sexe</th>
                                    <th class="sorting" tabindex="0" aria-controls="example1" rowspan="1" colspan="1" style="width: 268px;" aria-label="Grade: activate to sort column ascending">Grade</th>
                                    <th class="sorting" tabindex="0" aria-controls="example1" rowspan="1" colspan="1" style="width: 268px;" aria-label="Etablisement: activate to sort column ascending">Etablisement</th>
                                    <th class="sorting" tabindex="0" aria-controls="example1" rowspan="1" colspan="1" style="width: 301px;" aria-label="Telmob: activate to sort column ascending">Telmob</th>
                                    <th class="sorting" tabindex="0" aria-controls="example1" rowspan="1" colspan="1" style="width: 301px;" aria-label="Email: activate to sort column ascending">Email</th>
                                    <th class="sorting" tabindex="0" aria-controls="example1" rowspan="1" colspan="1" style="width: 268px;" aria-label="Dernierdepobtenu: activate to sort column ascending">Dernierdepobtenu</th>
                                    <th class="sorting" tabindex="0" aria-controls="example1" rowspan="1" colspan="1" style="width: 268px;" aria-label="Datedepobtenu: activate to sort column ascending">Datedepobtenu</th>
                                    <th class="sorting" tabindex="0" aria-controls="example1" rowspan="1" colspan="1" style="width: 268px;" aria-label="Etabdepobtenu: activate to sort column ascending">Etabdepobtenu</th>
                                    <th class="sorting" tabindex="0" aria-controls="example1" rowspan="1" colspan="1" style="width: 268px;" aria-label="Groupe: activate to sort column ascending">Groupe</th>
                                    <th class="sorting" tabindex="0" aria-controls="example1" rowspan="1" colspan="1" style="width: 154px;" aria-label="Actions: activate to sort column ascending">Action</th>
                                </tr>
                                {#
                                                                                                                                                                                                        <tr>
                                                                                                                                                                                                            <th>Id</th>
                                                                                                                                                                                                            <th>Cinenseigch</th>
                                                                                                                                                                                                            <th>Nom</th>
                                                                                                                                                                                                            <th>Prenom</th>
                                                                                                                                                                                                            <th>Nomjeunefille</th>
                                                                                                                                                                                                            <th>Datenaiss</th>
                                                                                                                                                                                                            <th>Lieunaiss</th>
                                                                                                                                                                                                            <th>Sexe</th>
                                                                                                                                                                                                            <th>Grade</th>
                                                                                                                                                                                                            <th>Etablisement</th>
                                                                                                                                                                                                            <th>Fonction</th>
                                                                                                                                                                                                            <th>Telmob</th>
                                                                                                                                                                                                            <th>Telfixe</th>
                                                                                                                                                                                                            <th>Email</th>
                                                                                                                                                                                                            <th>Dernierdepobtenu</th>
                                                                                                                                                                                                            <th>Datedepobtenu</th>
                                                                                                                                                                                                            <th>Etabdepobtenu</th>
                                                                                                                                                                                                            <th>Codestructure</th>
                                                                                                                                                                                                            <th>Created</th>
                                                                                                                                                                                                            <th>Updated</th>
                                                                                                                                                                                                            <th>Deletedat</th>
                                                                                                                                                                                                            <th>Actions</th>
                                                                                                                                                                                                        </tr>
                                
                                #}
                            </thead>
                            <tbody>
                                {% for enseignant_Chercheur in enseignant_Chercheurs %}
                                    <tr>
                                        <td><a href="{{ path('enseignant_chercheur_show', { 'id': enseignant_Chercheur.id }) }}">{{ enseignant_Chercheur.id }}</a></td>
                                        <td>{{ enseignant_Chercheur.cINEnseigCh }}</td>
                                        <td>{{ enseignant_Chercheur.nom }}</td>
                                        <td>{{ enseignant_Chercheur.prenom }}</td>

                                        <td>{% if enseignant_Chercheur.dateNaiss %}{{ enseignant_Chercheur.dateNaiss|date('Y-m-d') }}{% endif %}</td>
                                        <td>{{ enseignant_Chercheur.lieuNaiss }}</td>
                                        <td>{{ enseignant_Chercheur.sexe }}</td>
                                        <td>{{ enseignant_Chercheur.grade }}</td>
                                        <td>{{ enseignant_Chercheur.etablisement }}</td>

                                        <td>{{ enseignant_Chercheur.telMob }}</td>

                                        <td>{{ enseignant_Chercheur.eMail }}</td>
                                        <td>{{ enseignant_Chercheur.dernierDepObtenu }}</td>
                                        <td>{% if enseignant_Chercheur.dateDepObtenu %}{{ enseignant_Chercheur.dateDepObtenu|date('Y-m-d') }}{% endif %}</td>
                                        <td>{{ enseignant_Chercheur.etabDepObtenu }}</td>
                                        <td>{{ enseignant_Chercheur.team }}</td>


                                        <td>
                                            <ul>
                                                <li>
                                                    <a href="{{ path('enseignant_chercheur_show', { 'id': enseignant_Chercheur.id }) }}">show</a>
                                                </li>
                                                <li>
                                                    <a href="{{ path('enseignant_chercheur_edit', { 'id': enseignant_Chercheur.id }) }}"><i class="fa fa-pencil"></i> Modifier</a>
                                                </li>
                                            </ul>
                                        </td>
                                    </tr>
                                {% endfor %}
                            </tbody>
                            <tfoot>
                                <tr>
                                    <th rowspan="1" colspan="1">ID</th>
                                    <th rowspan="1" colspan="1">cINEnseigCh</th>
                                    <th rowspan="1" colspan="1">nom</th>
                                    <th rowspan="1" colspan="1">prenom</th>
                                    <th rowspan="1" colspan="1">dateNaiss</th>
                                    <th rowspan="1" colspan="1">lieuNaiss</th>
                                    <th rowspan="1" colspan="1">sexe</th>
                                    <th rowspan="1" colspan="1">grade</th>
                                    <th rowspan="1" colspan="1">etablisement</th>
                                    <th rowspan="1" colspan="1">telMob</th>
                                    <th rowspan="1" colspan="1">eMail</th>
                                    <th rowspan="1" colspan="1">dernierDepObtenu</th>
                                    <th rowspan="1" colspan="1">dateDepObtenu</th>
                                    <th rowspan="1" colspan="1">etabDepObtenu</th>
                                    <th rowspan="1" colspan="1">groupe</th>


                                    <th rowspan="1" colspan="1">Actions</th>
                                </tr>
                            </tfoot>
                        </table>
                    </div>
                </div>

            </div>
        </div>



    </div>
{% endblock %}
